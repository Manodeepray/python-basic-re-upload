#compound assignment +=
a=int(input("value of a"))
a+=int(input("value to add"))
print("a=")
a-=int(input("value to subtract"))
print("a=")
a/=int(input("value to divide"))
print("a=")
a*=int(input("value to multiply"))
print("a=")
a//=int(input("value to floordivision"))
print("a=")
a**=int(input("value to power"))
print("a=")